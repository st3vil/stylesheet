me.Sch_Comm = &acgt{ return [
    ['Abodo',0.7,&acgtRs{
        delete s&bgi;
    },'ift,D'],
    ['Peace',0.7,&acgtRs{
        s&hue ||= 30;
    },'ift,D'],
  // wai community
    ['noisemaking',0.6,&acgtRs{
         aha(R,'wou')({tw:'figaro',vipa:3});
         Ry&ha({waitel:2,p:py&l});
         Ry&ha({dwitty:'vael',flat:'others',vap:2})
