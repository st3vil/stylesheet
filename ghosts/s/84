                    acu $h,$k,$t;
                };
                C.sc.>$_ = join ",",@{$h.>$_ }
                    for sort keys %$h;
            }
            if (my $in = s&Tin) {
                s&Tins = ki $in;
                s&strung.Tin.Tins++;
            }
            $C
        };
        G&ind,$_,$ub;
    } @t;
    
