Four: | # savepoint/fork and wait
    my $parent = $$;
    if (my $pid = fork()) {
        1 && saygr "
            oooo
               o o
              o   o";
        
        $SIG{INT} = sub {
            sayre "Killing $pid...";
            kill 'KILL', $pid;
            delete $SIG{INT}
        };
        my $and;
        $SIG{HUP} = sub {
            $and = 1;
        };
        
        waitpid($pid,0);
        
        if ($and) {
            saybl "child wants restart!";
            return
            Rw waitme
        }
        
        saybl "Refork $t ? <";
        
        my $IN = \*STDIN;
        while (my $k = <$IN>) {
            chomp $k;
            return if G.c.loopst;
            #!G.reing && !$k && return 2;
            J.me = $k;
            say "__ $k";
            return 2;
        }
    }
    else {
        G.forked = $parent;
    }
