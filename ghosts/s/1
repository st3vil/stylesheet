            }
        };
        $y || die "no h: $s";
        
        my $uB = Gc&B || {t=>'nowhere',sc=>{A=>A.up}};
        my $B = {t=>Ds&of||"?",y=>{cv=>D.y.cv||''}};
        B.y.out = $uB;
        if (my $p = uB.y.in) {
            # pevious B-level call inside the uB call
            p.y.next = $B;
            B.y.prev = $p;
        }
        else {
            uB.y.in = $B;
        }
        Bs&y = $y;
