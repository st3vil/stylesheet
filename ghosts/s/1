    #c meaning of word/contenter
    # compiles for given args
    w:
        my $ar = {};
        # their $ar (+)
        if (ref $Me[0]) {
            my $ra = shift @Me;
            %$ar = %$ra;
        }
        # their args
        my @o = @Me;
        while (@Me) {
            my $k = shift @Me;
            @Me || die "w odd hash after $k: ".ki\@o;
            my $v = shift @Me;
            $ar.>$k = $v;
        }
        ar.__D && die "w __D deprecated: ".ki$ar;
        my $nar;
