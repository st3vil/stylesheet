pi/proc: | # picture in picture rock
    my ($d,$p) = $s =~ /^(.+)\/p\/(\d+)$/;
    $p || return
        n err=>6=>"No pid: $p",'%hs:254'
    s&dis = 3;
    y.at.dos = 'Continues';
    
    my $so = read_file("$s/so");
    my @lives = split ' ', $so;
    my $it = pop @lives;
    $so = join ' ', (map{/^(\d+)/}@lives), $it;
    s&so = $so;
    Rw Thrext so;
    n spc=>12=>' '
    
    n pid=>2=>$p,'%fs:7,hs:256'
    s&pid = $p;
    
    -e "/proc/$p" || return
        n gone=>2=>'*'=>'%hs:856'
    #n spc=>31=>' '
        
    
    my $pa = read_file("$s/pa");
    #n pa=>32=>{s=>$pa}=>'%hs:576'
