Calg: |
    Jc&tape = 'Dejo';
    
    n Chunkfall
            for (G&op,'t:~^[tT]' ) {
                m _.t=>{thy=>0.5}=>_c&s=>''
            }
            J.bowls < 5 && shift @$M for 1..J.bowls;
            for (@$M) {
                (_s&tag,_s&src) = ('img',delete(_c&s));
                _c&helum = 1.1;
            }            
            y.lum = 2 + 3 * J.bowls++ || return;
            J.bowls < 5 &&
            n do=>1=>"Fosci J.bowls\n",'%hs:345,fs:'.(5 * J.bowls)
            y.at.lumwas = y.lum;
            my $b = J.bowls;
            y.thi = 22 - $b * 3;
            y.thi *= -1 if y.thi < 1;
            y.thi = 2 if y.thi < 1;
            y.in = 3 - $b * 0.4;
            y.out = 6 + $b * 0.445;
            J.bowls = J.bowls - 10 if J.bowls > 12;
            y.at.ws = 3;
            y.imp++ ;#if J.bowls =
    
