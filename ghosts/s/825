Sendeth: |
    unless ( us&S ) {
        my $t = Js&sendguess || 'Woov';
        my $z = [values%{Js&TV||[]}];
        my $w = G&Ct,[w=>''=>{s=>$z}];
        G&elph,Elvis=>[opt=>''=>{s=>"$t:Go"=>etc=>{e=>$w}}];
        Js&say = '';
    }
    #
    %{Js&TV||[]} = ()
Go: | # Ghosteths: you receiving your mail
