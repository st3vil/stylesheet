pi/snakes: | # centered views of clutter
    my $z = delete s&z || die "No clutter";
    my @c = @$z;
    my $i = 0;
    for (@c) {
        _.t eq $s && last;
        $i++;
    }
    my $c = $c[$i];
    my $ii = $i;
    $i -= 4;
    $i++ while $i < 0;
    while (exists $c[$i]) {
        $_ = $c[$i];
        my $d = $ii - $i;
        $d *= -1 if $d < 0;
        my $fs = int(15 - $d ^ 2);
        $fs = 3 if $fs < 3;
        m _.t=>''=>_.t=>{fs=>$fs}
        $i++;
        last if $i > $ii + 4;
    }
    
    #A.V = 2;
    #n "sna ".C.t=>o1=>{s=>wdump 3, $c}
    
