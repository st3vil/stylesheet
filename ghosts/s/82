ElvisDoolify: |
    my $t = Rw Towering Ang
        || return;
    Rw HasWay;
    
    # wants to magnetise tape observation for anything
    
    if (c&Se == 1) {
        u Dooli=>'',<<'','%of:w'
            u Zpace
            Jc&elvis = 'Dejo';
            n Fergus =>-imigis=>'eye/fug','%qz,qs'
            my $tug = sub {
                m _.t=>''=>\'helum',{tag=>'img',src=>_c&s};
            };
            map{$tug->()} map{G&op,$_ } 't:~^[tT] spi:relim';
            m 'Fergus'=>-tiss
            map{$tug->()} map{G&op,$_ } 't:!~^[tT] spi:relim';
            y.lum = 2;
            y.thi = 20;
        t.e =
        n Dooli
        t.en->('sc lastK c s', "See tarps");
        t.e =
        n Dejo
