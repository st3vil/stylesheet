    me.sGhost = &acgtR{
        $Ha = me.cby(R,&b{ return
            bs&pushed && bs&pushed[0].t == 'lookup'
        })[0];
        if (Ha) {
            $pl = Hac&pushed[0];
            $Ghost = pls&there;
