                    #  single branch only, may loop
                    $parent = bs&current;
                    parent != current and currents&parent = parent
                    $ci = M.indexOf(current);
                    $pi = M.indexOf(parent);
                    ci == 0 && pi+1 == M.length and 'end to end'
                    else
                    pi == 0 && ci+1 == M.length and 'end to end'
                    else
                    ci == pi+1 and 'follows'
                    else {
                        M.splice(ci,1);
                        M.splice(pi,0,current);
                    }
                }
            };
