    #c Be.sR - adjust raised sticks
    Be.sc.R = 'sR';
    Be.sR = {
    ps_Dc:{Gens:{ohm:0}},
    play:&acgtRs{
        $c = {};
        $sc = ex({},s.sc);
        $k = delete sc.R;
        $z = delete sc.z;
        # R=\d+ could mean slopality
        k > 1 and c.Doming = k
        else {
            k == 1 and k = '1'
            typeof k != 'string' and throw "refR"
            k = k == '1' ? s.t : k;
            k = k.split('+');
            # R=+Save gives R%Save
            # R=Tool+etc makes r%Tool%etc
            k[0] == '' and k.shift(); c = R.sc
