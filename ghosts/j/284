                        s = Pc&s;
                        Rs&Decode.js(s);
                    }
                    else {
                        throw "Decode type "+type
                    }
                    s.y.P = P;
                    Pc&s = s;
                }
                catch (er) {
                    T.er = er;
                    de.set('l2','Error!');
                    de.set('l3',er);
                }
                Pc&s = s;
                s && !T.er and T.ok = 1
                
                if (T.ok && T.aft_waits) {
                    T.aft_waited = Gtime(T.aft_waits);
                    delete T.aft_waits;
                    $tcp = T.tcp;
                    tcp.set('l2',G&thime,T.aft_waited );
                }
            };
            
        
            #c Talks w/js/W to server
            Rs&Talks = {};
            Rs&Decode = {};
            Rs&Talks.w = &TCP{
                # < Babz mode: none, normal, BQ-inclusive
