FactorEase: | # arty maps
    u i
    # search last mo things top
    op:
        my $Wr = T.op || die "No Top to op";
        my $ub = !$s ? sub {
            my ($D,$d) = @_;
            $D if d.d == 2;
        } : do {
            my $C;
            C.t = 'oping';
            s&t = $s || die "No t to op";
            Rw conot $C;
            sub {
                my ($D,$d) = @_;
                $D if G&ip,s&ip,$D;
            }
        };
        my @ma = G&ind,$Wr,$ub,{noind=>1};
        @ma
