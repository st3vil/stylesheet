Door: |
    # interrogate process remotely
    Rw Tiot;
    Rw GiveTime;
    Js&ein.t.result.sMJ = &sMJje{
        1 && saybl "Returned: ".wdump 4, $s;
        #$s = 'sample';
        m res=>''=>$s
    };
    S
    if (!@{Js&M||[]}) {
        my $code = q/wdump 3, 4s&Jtv;/;
        my $re = [eval=>{}=>{s=>$code,returnto=>'Door'}];
        Rw Wudder $to=Text $s:_ for [Doory=>$re];
    }
    else {
        n result=>2=>result=>dis
        m $_ for @{Js&M};
    }
    
    
    
Doory: |
    #Rw Tiot;
    Js&ein.t.eval.sMJ = &sMJje{
        my $s = eval $s;
        return if $@;
        my $r =
        m Jtv=>''=>{s=>$s}
        if (my $to = ec&returnto) {
            my $s = [$to=>[result=>{}=>{s=>"Data: $s"}]];
            Rw Wudder $to $s;
            exit;
        }
    };
    S
    
    n on=>o1=>G.t,dis
    
    #m $_ for @{Js&M};
    
    

