        my $i = 
        n one=>-isness_3
        
    }
    
    my $h = int rand 9;
    n col=>3=>"Colour!","hs:${h}85"
Power: |
    # from https://github.com/cisco/ChezScheme/blob/master/examples/power.ss
    (define power
       (lambda (x n)
          (cond
             [(= n 0) 1]
             [(= n 1) x]
             [else
              (let ([q (quotient n 2)])
                 (* (power x q) (power x (- n q))))])))
    

