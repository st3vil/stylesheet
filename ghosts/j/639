FormShapes: | # stack certain shapes next to each other
    # ! hoee#ee?
    my @s;
    my @M = @$M;
    my $l;
    while (@M) {
        my $r = shift @M;
        if (ls&shape && ls&shape eq rs&shape) {
            push @s, [$l] unless @s && $s[-1]->[-1] eq $l;
            push @{$s[-1]}, $r;
        }
        $l = $r;
    }
    for my $s (@s) {
        my $i = 0;
        for my $C (@$s) {
            next unless $i++; # first one takes up space!
            s&ab = 1;
            s&left = 3 * $i;
        }
    }
    # etc etl eee!!
    G&give,Budge=>[formshapes=>''=>{s=>\@s}];


