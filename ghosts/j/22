        A&ws ||= 1;
        
        window.onerror = &e,r,o,l{
            console.log("Window error: ",e,r,o,l);
            return;
        };
        
        A.1 = A.2 = A.3 = A.4 = G&J,'Four';
        1s&N = []; // for A/C workload
        // 1s&z etc for showing state
        
        
        // our s&Jholder
        
        // location.search.match('^\\\?v=')
        
