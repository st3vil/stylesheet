NutAye: | # A without tubeyness
    u j
    n \'of:eye'
    # begin being, A
    # they can take over names within their network
    #   // slopa, as def NutCore
    #   A.om ||= A;
    #   A.om == A.up and A.mo = A;
    #   but such a name may need update as B (sub calls) revamp
    # also drops A.V for verbose, use T
    Aye: %acgt:s ov c sc
        if (s && typeof s == 'object') {
            A = s;
        }
        else {
            $V = A;
            A = $.extend({},A);
            A.t = s;
            A.y = A;
            A.y.cv = 0;
            A.c = {};
            A.sc = {};
            
            A.up = V;
            A&ip = (upA&ip||[]).slice(0);
            upA&ipi ||= 0;
            A&ip.push(upA&ipi++);
            A&sip = A&ip.join(' ');
            
            // trip A
        }
        c and $.extend(A.c,c);
        sc and $.extend(A.sc,sc);
        
        if (ov && ov*1 == ov) {
            while ov >= 1
                ov /= 10;
            A&ov = ov.toPrecision(4);
            G&thro;
        }
        return A
    # descript/mapself
NutSea: | # line etc into C
    u j
    n \'of:eye'
    # 
    # ReadLines, G&m, G&A
    Cye:
        // guess t y c sc
    # parse ONE C-like scrap of something, not a Line
    inC:
        C = s;
        if (C.constructor == Array) {
            C = {t:C[0],y:C[1],c:C[2],sc:C[3]};
            typeof C.c  === 'string' and C.c =  G&peel,C.c;
            typeof C.sc === 'string' and C.sc = G&peel,C.sc;
        }
        else
        if (C.constructor == Object &&
            !C.t && !C.y && !C.c && !C.sc) {
            $.each(C, &k,v{ G&$k,v }); T.not = 1;
        }
        else
        typeof C === 'string' and C = {c: {s: C}};
        typeof C.c === 'string' and C.c = {s: C.c};
        typeof C.y === 'string' || typeof C.y === 'number' and C.y = {cv: G&sca,C.y };
        !typeof C.t === 'string' and console.log('queda',C);
        C.c ||= {};
        C.sc ||= {};
        return C;
    
    # and a bit of xotila
NutTub: | # A tubeyness
    u j
    n \'of:eye'
