        T.D = z;
        return z.c.code(A,C,G,T,so,st,sh,sf,si);
    
    # look up A slope for the I/$s (embryo) or the I/$s/$t (part)
    t: %acgt:s t
        var z;
        $.each([1,2,3,4,5], &i,v{
            z and return;
