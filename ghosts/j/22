    G.Websock = &s{
        1 && say "Web is socked!";
        my $tx = $s->tx;
        push @$txm, $tx;
        my $addr = $tx->remote_address;
        Mojo::IOLoop->stream($tx->connection)->timeout(300000);
        $tx->max_websocket_size(512000);
        
        $s->on(message => &M,m{
            #}
            $m = encode_utf8($m);
            1 && sayyl "Message from $addr ".$m;
            
            my $ha = dig $m;
            $rec.>$ha ++ && sayre("Duplicate") && return
                Rw linkrement request_dupe;
