        _.t =~ /^\Q$t\E$/ ? do {
            $l = $_;
            0
        } :
        # shortens their t when in here, keeping y/c/sc
        _.t =~ /^\Q$t\E;?(.*)?$/ ? do {
            my $hash = {%$_,t=>$1};
        } : undef; # not here
