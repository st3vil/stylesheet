    t.days = 5;
    t.fracday = &s{
        $m = (''+s).match(/^(\d?\d)(\d\d)$/);
        !m and throw "not a time: "+s
        return (m[1] * (1/24)) + (m[2] * (1/1440))
    };
    t.daystart = t.fracday('0900');
    t.dayend = t.fracday('1700');
    t.Start = &{
