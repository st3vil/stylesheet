Clalia: | # drag n learn
    G&heading;
    # to make html+javascript that lives a life of its own
    
    n Classes -Class $s:<<''
    Engineering  4000
      seeing  439  %st:being,pow:24
        c s: |
          Lovely and sighted.
          Takes hold.
        Riddiford  %day:1,time:900-1040
        Riddiford  %day:3,time:1300-1450
        Taxiderm   %day:2,time:1300-1340&1400-1450
      triangle  469  %st:geometrica,pow:12
        c s: |
          Bring togs.
        Riddiford  %day:4,time:900-1040
        Riddiford  %day:3,time:1300-1450
        Taxiderm   %day:2,time:1300-1340&1400-1450
      weld  468  %st:geometrica,pow:9
        c s: |
          Ceramic?
        Riddiford  %day:1,time:900-1040
        Riddiford  %day:3,time:1300-1450
        Taxiderm   %day:2,time:1300-1340;1400-1450
    Art  8000
      weld  868  %st:enjoyment,pow:9
        c s: |
          But with plenty of slop.
        Riddiford  %day:1,time:900-1040
    
    # etc.
    n Week -Week s:40hour
pi/Class: |
    
    n t 1 $s:C.t
    
    if (typeof s == 'string') {
        # first fix '  ' into \t, editor swaps tabs for spaces
        # < editor should insert spaces and leave tabs alone
        # < Ctrl-tab inserts tab?
        $notwhile;
        s = s.split("\n").map(&s{
            !s.length and return
            #s.match(/Art/) and debugger;
            if (notwhile) {
                $m = s.match(/^\s+/);
                m && m[0].length > notwhile.length and return s
                notwhile = ''
            }
            notwhile = s.match(/^(\s+)c s:/);
            notwhile and notwhile = notwhile[1]; return s;
            
            $m = s.match(/^(.*?\S)  /);
            !m and throw "not a Line: "+s
            $line = s.slice(0,m[1].length);
            $etc = s.slice(m[1].length).split("  ").join("\t");
            etc and line += etc
            return line
        }).join("\n");
        s = G&readLines,s;
        
        # NOW, we have the top Cs, being groups/depart
        
    }
    
    $z = []; # things inside...
    if (s.constructor == Object) {
        # already are the thing...
        ss&z and z = ss&z
    }
    else { z = s }
    
    each iv z {
        m $v.t -Class $s:v %ml:1em,dis:block
    }
pi/Week: |
    ys&dis = 'block';
    
    $map = '1,2,3,4,5x900-1700';
    s != '40hour' and throw "what kind of week is "+s
    
    
    

