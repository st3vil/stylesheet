                .css('position','absolute')
                .css('width','100%')
                .css('top',(starts*100)+'%')
                .css('height',(endures*100)+'%')
                .css('background','rgba(176,140,130,0.6)');
            plot.text(starts+' - '+endures);
            plot.attr('class','potential');
            plot.appendTo(day);
            
        }
    };
    
    

    
