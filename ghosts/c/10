Detailtest: |
    for (grep {$_ eq '2'} A.us&time) {
        my ($j) = A.c&J;
        is($j, $J, "J is AcJ");
        my ($e) = A.e&C;
        is(ref $e, "HASH", "e is hash");
        ok(e.t&&e.y&&e.c, "e is tyc");
