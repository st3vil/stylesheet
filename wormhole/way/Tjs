vid:
    s&Tac == 0 and return
    ~gotviddy: s, C.t, C
    a.putontv = &l{
        var m = l.match('^\\\?v=([^&]+)(&.+)?$');
        if (m) {
            l = m[1]+'?steve=steve';
            m[2] and l += m[2];
            l = 'https://www.youtube.com/embed/'+l;
        }
        G&vid,l;
    };
    $l = typeof s == 'string' && s || G&pull,s.t;
    $lt = l.split(' # ');
    l = lt[0];
    $t = lt[1];
    ~vid: t

    $c = {};
    $re = {
        in:'[ti]',
        out:'o',
        len:'(len|secs)',
        any:'io'
    };
    $.each(re, &k,v{
        $se = l.match('&'+v+'=(\\d+)');
        !se and return;
        se = se.pop();
        se.match("^\\d+$") and se = se * 1;
        c[k] = se;
    });

    if (c.any) {
        $any = c.any.split(',');
        any = G&shuff,any;
        $one = any.shift();
        $is = one.split('-');
        c.in = is[0];
        is[1] and c.out = is[1];
    }

    $hy = s&Tac - 4;
    if (hy > 0 && c.len && (!c.in || !c.out)) {
        $into = &nd{
            while (nd > (89 / hy)) {
                nd *= 0.3
            }
            return nd
        };

        $ent = 1*('0.'+(""+a.nw()).match('.+(\\d{2})\\.')[1]);
        ent ||= elvis;

        c.out && !c.in and c.in = c.out - into(c.in);
        c.in ||= Math.round(c.len * ent);
        c.in && !c.out and c.out = c.in + into(c.len-c.in);
    }

    if (c.in) {
        l = l+'&start='+c.in;
    }
    if (c.out) {
        $nd = c.out - c.in;
        ~vidoutin: nd, t
        nd *= 1000;
        a.yl(nd, &{ 
            A.om.lastvid != l and return console.log("Not last vid: "+t);
            G&vid,s ;
        });
    }
    $e = c;
    $c = {el:2};
    $sc = {geo:'a1 0.1361 0.55 -0.3734'};
    if (l.match('^\/')) {
        c.ht = 1;
        c.s = '<video width="600px" autoplay controls><source src="'+l+'"></video>';
    }
    else {
        l = l + "&autoplay=1";
        sc.z = [['i',4,{el:2},{ifr:l}]];
    }
    A.om.lastvid = l;
    c.slow = 3;
    sc.fadeonin = 3;
    a.om(A.om,[C.t+'eo',4,c,sc],a,{});

