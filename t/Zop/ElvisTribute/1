  - "  spc 0.1 el=2 c { s+# to take up space\\n } sc { acgt+s act+1 of+L v+yspc }"
  - "  u 0.1 el=2 c { s+# to take space up til\\n } sc { acgt+s act+1 of+L v+yu }"
  - "  in 0.12 el=2 c { s+C.y.n = delete C.y...<0 } sc { acgt+s act+1 of+L v+yin vatis+!ref }"
  - "  out 0.12 el=2 c { s+C.y.u = delete C.y...<0 } sc { acgt+s act+1 of+L v+yout vatis+!ref }"
  - "  M 0.16 el=1 c { s+return if C.y.a; # ..<400 } sc { acgt+s act+1 of+mo tldo+nogindex:y:L vor+cM }"
