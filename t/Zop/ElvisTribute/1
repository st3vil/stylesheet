  - "  bal 0.3 el=2 c { s+my $M = c&M;\\nRw Mod..<0 } sc { acgt+s act+1 of+L v+ybal }"
  - "  refit 0.4 el=2 c { s+#local A.V = 2;\\nmy ..<1800 } sc { acgt+s act+1 of+L v+yres }"
  - "  barel 0.5 el=2 c { s+my $y = C.y;\\nmy $to..<0 } sc { acgt+s act+1 of+L v+cM }"
  - "  many 0.6 el=2 c { s+my $y = C.y;\\ny.mu =..<300 } sc { acgt+s act+1 of+a }"
  - "  lum 0.7 el=1 c { s+my $M = c&M || die ..<200 } sc { acgt+s act+1 of+L v+ylum }"
  - "  res 0.7 el=2 c { s+ref $res || return;..<100 } sc { acgt+s act+1 of+L v+yres }"
  - "  ground 0.8 el=2 c { s+my $M = c&M || die ..<500 } sc { acgt+s act+1 of+L }"
